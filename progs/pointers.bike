func putc(u8 char) -> u8 {
	u8 val = char; // no optimization >:(
	asm#("
	mov rax, 1
	mov rdi, 1
	mov rsi, rsp
	mov rdx, 1
	syscall
	")

	return val;
}

func main() {
	str ptr = "String";
	print#("S ascii: ");

	i32 idx = 0;
	loop {
		if idx >= 6 { break };

		str offset = ptr + |u64|idx;
		*offset = *offset + 1;

		putc(*offset);

		idx = idx + 1;
	};
}
