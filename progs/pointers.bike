func putc(u8 char) -> u8 {
	u8 val = char; // no optimization >:(
	asm#("
	mov rax, 1
	mov rdi, 1
	mov rsi, rsp
	mov rdx, 1
	syscall
	")

	return val;
}

func main() {
	str ptr = "String";
	print#("S ascii: ", *ptr);

	i32 idx = 0;
	loop {
		if idx >= 6 { break };
		
		putc(*(ptr + idx));

		idx = idx + 1;
	};
}
