func main() -> void {
    let i32 j = 0;
    print#("Cartesian product: \n");

    loop {
        if j >= 10 { break };

        let i32 i = 0;
        loop {
            if i >= 10 { break };

            print#("( ", i, ", ", j, " )\n");
            i = i + 1;
        };
        j = j + 1;
    };

    print#("`i` is still valid here: ", i, "\n");
} 
