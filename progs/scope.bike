func test(i32 a, i32 b) -> i32 {
	let i32 addition = a+b;
	let i32 multiplication = a*b;

	return addition + multiplication;
}

func main() {
	let i32 function = 10;

	{
		{
			let i8 inner = 2;

			// shadow
			let i32 inner = <i32>inner;
		};

		// uses the same space as the scope above
		let i32 inner = 5;

		print#("inner: ", inner, "\n");
	};

	print#("function: ", function, "\n");
	
}
